{
  "name": "debugSqsEvent",
  "version": "1.0.0",
  "description": "Serverless aws-nodejs-typescript template",
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "esbuild handler.ts --bundle --minify --sourcemap --platform=node --target=es2020 --outfile=dist/handler.js",
    "postbuild": "cd dist && zip -r lambda.zip handler.js*",
    "test": "jest",
    "test:coverage": "npm run test -- --coverage --silent --testLocationInResults --ci --json --outputFile=\"report.json\""
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "@aws-lambda-powertools/logger": "^2.0.0",
    "@aws-lambda-powertools/metrics": "^2.0.0",
    "@aws-lambda-powertools/tracer": "^2.0.0",
    "@middy/core": "^6.0.0",
    "@middy/http-json-body-parser": "^6.0.0",
    "ulid": "^2.3.0"
  },
  "devDependencies": {
    "@serverless/typescript": "^3.0.0",
    "@types/aws-lambda": "^8.10.71",
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.18",
    "aws-sdk": "^2.1295.0",
    "esbuild": "^0.24.0",
    "esbuild-node-tsc": "^2.0.5",
    "jest": "^29.3.1",
    "json-schema-to-ts": "^2.12.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^5.0.0"
  },
  "author": "smockoro",
  "license": "MIT"
}
