{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "timezone": "Asia/Tokyo",
  "automerge": true,
  "branchConcurrentLimit": 0,
  "dependencyDashboard": false,
  "packageRules": [
    {
      "groupName": "express-docker",
      "matchPaths": ["express-docker/"]
    },
    {
      "groupName": "fastify-docker",
      "matchPaths": ["fastify-docker/"]
    },
    {
      "groupName": "aws-serverless",
      "matchPaths": ["aws-serverless/"]
    }
  ]
}
